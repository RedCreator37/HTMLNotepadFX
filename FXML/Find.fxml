<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="149.0" prefWidth="344.0" xmlns="http://javafx.com/javafx/8.0.121"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
    <Button layoutX="172.0" layoutY="96.0" mnemonicParsing="false" onAction="#find" prefHeight="30.0" prefWidth="79.0"
            text="Search"/>
    <Label layoutX="14.0" layoutY="14.0" text="Search for:"/>
    <TextField fx:id="searchInput" layoutX="14.0" layoutY="41.0" prefHeight="30.0" prefWidth="317.0"
               promptText="Enter search term..."/>
    <Button fx:id="findClose" layoutX="259.0" layoutY="96.0" mnemonicParsing="false" onAction="#findClose"
            prefHeight="30.0" prefWidth="72.0" text="Cancel"/>
</AnchorPane>
